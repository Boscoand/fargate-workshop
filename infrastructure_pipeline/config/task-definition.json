{
    "family": "trivia-backend",
    "containerDefinitions": [
        {
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ],
            "image": "<PLACEHOLDER>",
            "essential": true,
            "name": "api"
        }
    ],
    "cpu": "256",
    "memory": "512",
    "taskRoleArn": "PLACEHOLDER_TASK_ROLE",
    "executionRoleArn": "PLACEHOLDER_EXECUTION_ROLE",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc"
}
